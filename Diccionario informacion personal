# Crear un diccionario con información personal
informacion_personal = {
    "nombre": "Alejandro Sanchez",
    "edad": 25,
    "ciudad": "Quito",
    "profesion": "Licenciado"
}

# Acceder y modificar la ciudad
informacion_personal["ciudad"] = "Santo Domingo"

# Agregar una nueva clave-valor para la profesión
informacion_personal["profesion"] = "educación"

# Verificar si la clave "telefono" existe, si no, agregarla
if "telefono" not in informacion_personal:
    informacion_personal["telefono"] = "0972639886"

# Eliminar la clave "edad"
del informacion_personal["edad"]

# Imprimir el diccionario final
print(informacion_personal)
